{
  "name": "babel-plugin-alias",
  "version": "1.2.1",
  "description": "This is an alias conversion babel plugin",
  "main": "dist/index.js",
  "author": "drdevelop",
  "repository": {
    "type": "git",
    "url": "https://github.com/drdevelop/babel-plugin-alias.git"
  },
  "keywords": [
    "babel plugin",
    "typescript",
    "import",
    "module",
    "alias",
    "compiler"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drdevelop/babel-plugin-alias/issues"
  },
  "homepage": "https://github.com/drdevelop/babel-plugin-alias#readme",
  "scripts": {
    "prepare": "husky install",
    "dev": "babel-node --extensions .ts,.js ./core/index --config-file ./babel.config.js",
    "build": "tsc",
    "prepublishOnly": "npm run test",
    "pub": "npm run build && standard-version && npm publish",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "typescript": ">=1.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist",
    "package.json",
    "yarn.lock"
  ]
}
